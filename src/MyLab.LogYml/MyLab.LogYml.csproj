    <RootNamespace>MyLab.LogYml</RootNamespace>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Andrey O.</Authors>
    <Copyright>Copyright (c) 2018 Andrey Osintsev &lt;ozzy.ext@gmail.com&gt;</Copyright>
    <RepositoryUrl>https://github.com/ozzy-ext/mylab-log-yml.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/ozzy-ext/mylab-log-yml</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
